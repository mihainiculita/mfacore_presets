<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
    author="Michael Niculita"
    description="Tags for the Mfacore."
    shortdescription="Mfacore tags"
    link="http://mfatech.org"
    version="0.1">
    
  <group name="Lines" icon="transport_bicycle.png">
    <item name="CycLaTrack" icon="transport_bicycle.png">
      <link href="http://wiki.openaviationmap.org/wiki/index.php/Navaid" />
      <label text="Edit Cycle Lane/Track Attributes" />
      <space />
      <text key="TYPE" text="Type" default="Cycle Lane/Track ID" />
      <text key="CLT_ID" text="Cycle Lane/Track ID" />
      <checkgroup columns="2">
        <check key="CLT_CARR"  text="On / Off Carriageway"  default="on"  />
        <check key="CLT_SEGREG" text="Segregated lane / track" />
      </checkgroup>
      <check key="CLT_STEPP" text="Stepped lane / track" />
      <check key="CLT_PARSEG"  text="Partially segregated lane / track" />
      <check key="CLT_SHARED" text="Shared lane or footway" />
      <check key="CLT_MANDAT" text="Mandatory cycle lane" />
      <check key="CLT_ADVIS"  text="Advisory cycle lane" />
      <check key="CLT_PRIORI" text="Cycle lane/track priority" />
      <check key="CLT_CONTRA" text="Contraflow lane/track" values_from="getDateTime()"/>
      <check key="CLT_BIDIRE" text="Bi-directional" />
      <check key="CLT_CBYPAS" text="Cycle bypass" />
      <check key="CLT_BBYPAS"  text="Bus stop bypass" />
      <check key="CLT_PARKR" text="Park route" />
      <check key="CLT_WATERR" text="Waterside route" />
      <check key="CLT_FPTIME" text="Full-time / Part-time" />
      <text key="CLT_ACCESS" text="Access times" />
      <text key="CLT_SVDATE" text="Survey Date" />
      <text key="CLT_SVTIME" text="Survey Time" />
      <text key="CLT_SVNAME" text="Surveyor Name" />
      <text key="CLT_QADATE" text="Field Survey QA Date" />
      <text key="CLT_QANAME" text="Field Survey QA by" />
      <text key="CLT_DPNO" text="Data package number" />
      <text key="CLT_UPDATE" text="Update date" />
      <text key="CLT_UPNAME" text="Update by" />
      <text key="COMMENTS" text="Comment" />
      <combo key="CLT_COLOUR" values="None,Green,Red,Blue,Buff,Other" text="Colour" />
    </item>
    
    <item name="ASL" icon="styles/standard/transport/way/way_secondary.svg">
      <link href="http://wiki.openaviationmap.org/wiki/index.php/Navaid" />
      <label text="Edit ASL Attributes" />
      <space />
      <text key="TYPE" text="Type" default="ASL" />
      <text key="ASL_ID" text="Advanced Stop Line ID" />
      <check key="ASL_FDR"  text="Feeder lane	" />
      <check key="ASL_FDRLFT" text="Feeder lane on left" />
      <check key="ASL_FDCENT" text="Feeder Lane in centre" />
      <check key="ASL_FDRIGH"  text="Feeder lane on right" />
      <check key="ASL_SHARED" text="Shared nearside lane" />
      <text key="ASL_SVDATE" text="Survey Date" />
      <text key="ASL_SVTIME" text="Survey Time" />
      <text key="ASL_SVNAME" text="Surveyor Name" />
      <text key="ASL_QADATE" text="Field Survey QA Date" />
      <text key="ASL_QANAME" text="Field Survey QA by" />
      <text key="ASL_DPNO" text="Data package number" />
      <text key="ASL_UPDATE" text="Update date" />
      <text key="ASL_UPNAME" text="Update by" />
      <text key="COMMENTS" text="Comment" />
      <combo key="ASL_COLOUR" values="None,Green,Red,Blue,Buff,Other" text="Surface Colour" />
    </item>
    
    <item name="Crossing" icon="styles/standard/transport/way/way_secondary.svg">
      <label text="Edit Crossing Attributes" />
      <link href="http://wiki.openaviationmap.org/wiki/index.php/Obstacle" />
      <space />
      <text key="TYPE" text="Type" default="ASL" />
      <text key="CRS_ID" text="Crossing ID" />
      <check key="CRS_SIGNAL"  text="Signal controlled crossing" />
      <check key="CRS_SEGREG" text="Segregated cycles and pedestrians" />
      <check key="CRS_CYGAP" text="Cycle gap" />
      <check key="CRS_PEDEST"  text="Pedestrian Only Crossing" />
      <check key="CRS_LEVEL" text="Level Crossing" />
      <text key="CRS_SVDATE" text="Survey Date" />
      <text key="CRS_SVTIME" text="Survey Time" />
      <text key="CRS_SVNAME" text="Surveyor Name" />
      <text key="CRS_QADATE" text="Field Survey QA Date" />
      <text key="CRS_QANAME" text="Field Survey QA by" />
      <text key="CRS_DPNO" text="Data package number" />
      <text key="CRS_UPDATE" text="Update date" />
      <text key="CRS_UPNAME" text="Update by" />
      <text key="COMMENTS" text="Comment" />
      <combo key="CRS_COLOUR" values="None,Green,Red,Blue,Buff,Other" text="Surface Colour" />
    </item>

    <item name="RestrRout" icon="styles/standard/transport/way/way_secondary.svg">
      <label text="Edit Restricted route Attributes" />
      <link href="http://wiki.openaviationmap.org/wiki/index.php/Aerodrome" />
      <space />
      <text key="TYPE" text="Type" default="Restricted route" />
      <text key="RES_ID" text="Restricted route ID" />
      <check key="RES_PEDEST"  text="Pedestrian only route" />
      <check key="RES_BRIDGE" text="Pedestrian bridge" />
      <check key="RES_TUNNEL" text="Pedestrian tunnel" />
      <check key="RES_STEPS"  text="Steps" />
      <check key="RES_LIFT" text="Lift" />
      <text key="RES_COLOUR" text="Surface colour" />
      <text key="RES_SVDATE" text="Survey Date" />
      <text key="RES_SVTIME" text="Survey Time" />
      <text key="RES_SVNAME" text="Surveyor Name" />
      <text key="RES_QADATE" text="Field Survey QA Date" />
      <text key="RES_QANAME" text="Field Survey QA by" />
      <text key="RES_DPNO" text="Data package number" />
      <text key="RES_UPDATE" text="Update date" />
      <text key="RES_UPNAME" text="Update by" />
      <text key="COMMENTS" text="Comment" />
      <combo key="RES_COLOUR" values="None,Green,Red,Blue,Buff,Other" text="Colour" />
    </item>
  </group>
  
  <group name="Points" icon="styles/standard/transport/way/way_secondary.svg">
    <item name="Sign/Symb" icon="styles/standard/transport/way/way_secondary.svg">
      <text key="TYPE" text="Type" default="Singal or Symbol" />
      <text key="SS_ID" text="Signage ID" />
      <check key="SS_ROAD"  text="Road marking" />
      <check key="SS_PATCH" text="Coloured patch on surface" />
      <check key="SS_FACING" text="Facing off-side" />
      <text key="SS_DIAG"  text="Sign diagram number" />
      <check key="SS_NOCYC" text="No cycling" />
      <check key="SS_NOVEH" text="No vehicles" />
      <check key="SS_CIRC" text="Circular" />
      <check key="SS_EXEMPT" text="Exemption" />
      <check key="SS_NOLEFT" text="No left turn exception" />
      <check key="SS_NORIGH" text="No right turn exception" />
      <check key="SS_LEFT" text="Compulsory turn left exception" />
      <check key="SS_RIGHT" text="Compulsory turn right exception" />
      <check key="SS_NOEXCE" text="No straight ahead exception	" />
      <check key="SS_DISMOU" text="Cyclists dismount" />
      <check key="SS_END" text="End of Route" />
      <check key="SS_CYCSMB" text="Cycle symbol" />
      <check key="SS_PEDSMB" text="Pedestrian symbol" />
      <check key="SS_BUSSMB" text="Bus symbol" />
      <check key="SS_SMB" text="Other vehicle symbol" />
      <check key="SS_LNSIGN" text="Line on sign" />
      <check key="SS_ARROW" text="Direction arrow	" />
      <check key="SS_DESTN" text="Destination" />
      <combo key="SS_NRCOL" values="None,Green,Red,Blue,Buff,Other" text="Colour of route number patc" />
      <check key="SS_NCN" text="National Cycle Network" />
      <check key="SS_LCN" text="London Cycle Network" />
      <check key="SS_SUPERH" text="Cycle Superhighway" />
      <check key="SS_QUIETW" text="Quietway" />
      <check key="SS_GREENW" text="Greenway" />
      <check key="SS_ROUTEN" text="Route Number" />
      <check key="SS_ACCESS" text="Access times" />
      <text key="SS_SVTIME" text="Survey Time" />
      <text key="SS_SVNAME" text="Surveyor Name" />
      <text key="SS_QADATE" text="Field Survey QA Date" />
      <text key="SS_QANAME" text="Field Survey QA by" />
      <text key="SS_DPNO" text="Data package number" />
      <text key="SS_UPDATE" text="Update date" />
      <text key="SS_UPNAME" text="Update by" />
      <text key="COMMENTS" text="Comment" />
    </item>
    
    <item name="Signal" icon="styles/standard/transport/way/way_secondary.svg">
      <key key="vfrreportingpoint" value="yes" />
      <label text="Edit VFR Reporting Point" />
      <link href="http://wiki.openaviationmap.org/wiki/index.php/VfrReportingPoint" />
      <space />

      <!-- NAMES AND REF -->
      <text key="name" text="Name" />
      <text key="longname" text="Long name" />
      <text key="ref"  text="Reference number" description="The official reference number of this entity."/>
      <text key="url" text="URL" description="An internet URL to where you got information about this feature" />

      <!-- HEIGHT -->
      <text key="height" text="Height" description="Height (number)"/>
      <combo key="height:unit" text="Height Unit" values="ft,fl,m" description="Height Unit (feet, flight level or meters)" />
      <combo key="height:class" text="Height Class" values="amsl,agl" description="Height Class (Above Mean Sea Level, Above Ground Level)" />

      <!-- COMMUNICATION -->
      <text key="comm:ctrl" text="CTRL freq." description="Frequency to the authority ascosiated with this control point." />
    </item>
    
